{
  "manifest_version": 2,

  "name": "Share Mark",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "omnibox": { "keyword" : "sm" },
  "permissions": [
    "activeTab", "bookmarks"
  ]
}
